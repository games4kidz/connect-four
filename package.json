    "mithril"
  ],
  "author": "Caleb Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caleb531/connect-four/issues"
  },
  "homepage": "https://github.com/caleb531/connect-four#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "express": "^4.16.2",
    "fastclick": "^1.0.6",
    "mithril": "github:MithrilJS/mithril.js#deaf01c8e96446c97f31826b1d04ab4a4e757718",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.0.4",
    "tiny-emitter": "^1.1.0",
    "underscore": "^1.8.3",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.6.0",
